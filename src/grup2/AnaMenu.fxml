   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button id="btnGiris" mnemonicParsing="false" onMouseClicked="#btnGirisOnclick" prefHeight="70.0" prefWidth="200.0" text="Kullanıcı Girişi" GridPane.columnIndex="1">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button id="btnKayitOl" mnemonicParsing="false" onMouseClicked="#btnKayitOlOnclick" prefHeight="70.0" prefWidth="200.0" text="Kullanıcı Kaydı" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button id="btnAyarlar" mnemonicParsing="false" prefHeight="70.0" prefWidth="200.0" text="Ayarlar" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button id="btnCikis" mnemonicParsing="false" onMouseClicked="#btnCikisOnclick" prefHeight="70.0" prefWidth="200.0" text="Çıkış" GridPane.columnIndex="1" GridPane.rowIndex="3">
