<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" prefHeight="480.0" prefWidth="600.0" vgap="10" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grup2.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
